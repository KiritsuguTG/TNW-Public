<RedAlertActiveModel xmlns="uri:ea.com:eala:asset" xmlns:xi="http://www.w3.org/2001/XInclude">
    <PlayerUpgradeSpecialPower
        id="ModuleTag_SpecialPower_ActiveRedAlert"
        SpecialPowerTemplate="SpecialPower_ActiveRedAlert"
        AvailableAtStart="true">
        <Upgrade>UpgradeUprising</Upgrade>
        <Upgrade>PlayerUnitUpgradeToHeroic</Upgrade>
        <Upgrade>UpgradeUprisingCantUse</Upgrade>
    </PlayerUpgradeSpecialPower>
    <ObjectCreationUpgrade
        id="ModuleTag_ObjectUpgradeUprising"
        GrantUpgrade="ObjectUpgradeUprising">
        <TriggeredBy>UpgradeUprising</TriggeredBy>
    </ObjectCreationUpgrade>
    <ObjectCreationUpgrade
        id="ModuleTag_NotObjectUpgradeUprising"
        RemoveUpgrade="ObjectUpgradeUprising">
        <TriggeredBy>NotUpgradeUprising</TriggeredBy>
    </ObjectCreationUpgrade>
    <ObjectCreationUpgrade
        id="ModuleTag_RemoveUnitUpgradeToHeroic"
        ThingToSpawn="RemoveUnitUpgradeToHeroic">
        <TriggeredBy>PlayerUnitUpgradeToHeroic</TriggeredBy>
    </ObjectCreationUpgrade>
    <RemoveUpgradeUpgrade
        id="ModuleTag_RemoveRedAlert">
        <TriggeredBy>NotUpgradeUprising</TriggeredBy>
        <UpgradeToRemove>UpgradeUprising</UpgradeToRemove>
    </RemoveUpgradeUpgrade>
    <RemoveUpgradeUpgrade
        id="ModuleTag_RemoveNotReadyRedAlert">
        <TriggeredBy>UpgradeUprising</TriggeredBy>
        <UpgradeToRemove>NotUpgradeUprising</UpgradeToRemove>
    </RemoveUpgradeUpgrade>
    <RemoveUpgradeUpgrade
        id="ModuleTag_RemoveUpgradeUprisingNoUse">
        <TriggeredBy>UpgradeUprisingCantUse</TriggeredBy>
        <UpgradeToRemove>UpgradeUprisingNoUse</UpgradeToRemove>
    </RemoveUpgradeUpgrade>
    <ModelConditionUpgrade
        id="ModuleTag_UpgradeUprisingAddConditionFlags"
        AddConditionFlags="HEALTH_PERCENT_100">
        <TriggeredBy>UpgradeUprising</TriggeredBy>
    </ModelConditionUpgrade>
</RedAlertActiveModel>