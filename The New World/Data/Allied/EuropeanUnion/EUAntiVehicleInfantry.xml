				<AnimationState
					ParseCondStateType="PARSE_DEFAULT"
					Flags="RESTART_ANIM_WHEN_COMPLETE"
					AllowRepeatInRandomPick="true"
					StateName="STATE_bored">
					<Animation
						AnimationName="AUAntiVehicleInfantryII_BIDA"
						AnimationMode="ONCE"
						AnimationBlendTime="7"
						AnimationPriority="100"
						AnimationSpeedFactorMin="0.8"
						AnimationSpeedFactorMax="1.2" />
					<Animation
						AnimationName="AUAntiVehicleInfantryII_BIDD"
						AnimationMode="ONCE"
						AnimationPriority="100"
						AnimationBlendTime="15"
						AnimationSpeedFactorMin="0.8"
						AnimationSpeedFactorMax="1.2" />
					<Animation
						AnimationName="AUAntiVehicleInfantryII_BIDC"
						AnimationMode="ONCE"
						AnimationPriority="100"
						AnimationBlendTime="15"
						AnimationSpeedFactorMin="0.8"
						AnimationSpeedFactorMax="1.2" />

					<Script>
						Prev = CurDrawablePrevAnimationState()
						if Prev == "STATE_Suppressed" then
						CurDrawableSetTransitionAnimState("TRANS_fromSuppressed")
						elseif Prev == "STATE_SelectedPose" then
						CurDrawableSetTransitionAnimState("TRANS_SelectedToIdle")
						elseif Prev == "STATE_stunned_flailing" then
						CurDrawableSetTransitionAnimState("STATE_stunned_landing")
						end
						if Prev == "STATE_Parachuting" then
						CurDrawableSetTransitionAnimState("TRANS_fromParachuting")
						end
					</Script>
				</AnimationState>

