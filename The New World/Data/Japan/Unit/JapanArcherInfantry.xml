			UnitBuilderStandardCombatUnit="true"
			ConquerMetricsOverrideDPS="50"
			ConquerMetricsDpsBonusAgainstDefenses="2.0"
			ConquerMetricsOverrideDamageType="GUN"
			ConquerMetricsOverrideAntiMask="ANTI_INFANTRY ANTI_AIRBORNE_INFANTRY ANTI_AIRBORNE_VEHICLE"
			ConquerMetricsOverrideForbiddenAntiMask="ANTI_VEHICLE ANTI_WATER"/>
		<Draws>
			<ScriptedModelDraw
